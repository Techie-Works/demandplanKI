{
    "_version": "1.59.0",
    "sap.app": {
        "id": "kibi",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:alp",
            "version": "1.13.1",
            "toolsId": "3f2a170f-8c0f-41d2-87bb-0378b1cc46bd"
        },
        "dataSources": {
            "mainService": {
                "uri": "/odata/v4/bi/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "odataVersion": "4.0"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.122.2",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.ushell": {},
                "sap.fe.templates": {},
                "sap.f": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "kibi.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true
                }
            },
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            }
        },
        "resources": {
            "css": []
        },
        "routing": {
            "routes": [
                {
                    "pattern": ":?query:",
                    "name": "BIentityList",
                    "target": [
                        "BIentityList"
                    ]
                },
                {
                    "pattern": "BIentity({key}):?query:",
                    "name": "BIentityObjectPage",
                    "target": [
                        "BIentityList",
                        "BIentityObjectPage"
                    ]
                }
            ],
            "targets": {
                "BIentityList": {
                    "type": "Component",
                    "id": "BIentityList",
                    "name": "sap.fe.templates.ListReport",
                    "options": {
                        "settings": {
                            "contextPath": "/BIentity",
                            "variantManagement": "Control",
                            "navigation": {
                                "BIentity": {
                                    "detail": {
                                        "route": "BIentityObjectPage"
                                    }
                                }
                            },
                            "controlConfiguration": {
                                "@com.sap.vocabularies.UI.v1.LineItem": {
                                    "tableSettings": {
                                        "type": "AnalyticalTable",
                                        "selectionMode": "Auto"
                                    },
                                    "columns": {
                                        "Customcolumn": {
                                            "header": "Mycustomcolumn",
                                            "width": "20",
                                            "position": {
                                                "anchor": "DataFieldForAnnotation::DataPoint::output5",
                                                "placement": "After"
                                            },
                                            "template": "kibi.ext.fragment.Customcolumn"
                                        }
                                    }
                                },
                                "@com.sap.vocabularies.UI.v1.SelectionFields": {
                                    "filterFields": {
                                        "Section": {
                                            "visualFilter": {
                                                "valueList": "com.sap.vocabularies.Common.v1.ValueList#visualFilter"
                                            }
                                        },
                                        "Status": {
                                            "visualFilter": {
                                                "valueList": "com.sap.vocabularies.Common.v1.ValueList#visualFilter"
                                            }
                                        },
                                        "Demand": {
                                            "visualFilter": {
                                                "valueList": "com.sap.vocabularies.Common.v1.ValueList#visualFilter"
                                            }
                                        }
                                    },
                                    "layout": "CompactVisual",
                                    "initialLayout": "Visual"
                                }
                            },
                            "views": {
                                "paths": [
                                    {
                                        "primary": [
                                            {
                                                "annotationPath": "com.sap.vocabularies.UI.v1.PresentationVariant"
                                            }
                                        ],
                                        "secondary": [
                                            {
                                                "annotationPath": "com.sap.vocabularies.UI.v1.LineItem#bitable"
                                            }
                                        ],
                                        "defaultPath": "both"
                                    }
                                ]
                            },
                            "initialLoad": "Enabled"
                        }
                    },
                    "controlAggregation": "beginColumnPages",
                    "contextPattern": ""
                },
                "BIentityObjectPage": {
                    "type": "Component",
                    "id": "BIentityObjectPage",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "editableHeaderContent": false,
                            "contextPath": "/BIentity",
                            "sectionLayout": "Page"
                        }
                    },
                    "controlAggregation": "midColumnPages",
                    "contextPattern": "/BIentity({key})"
                }
            },
            "config": {
                "flexibleColumnLayout": {
                    "defaultTwoColumnLayoutType": "TwoColumnsBeginExpanded",
                    "defaultThreeColumnLayoutType": "ThreeColumnsMidExpanded"
                },
                "routerClass": "sap.f.routing.Router"
            }
        },
        "rootView": {
            "viewName": "sap.fe.templates.RootContainer.view.Fcl",
            "type": "XML",
            "async": true,
            "id": "appRootView"
        }
    },
    "sap.fiori": {
        "registrationIds": [],
        "archeType": "analytical"
    }
}
